{
    "name": "papiyas/validator",
    "type": "library",
    "description": "A validator which auto check and throw an common-api return on errors",
    "keywords": [
        "api",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "papiyas/api": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Papiyas\\Validator\\ValidatorServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
 	        "Papiyas\\Validator\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
